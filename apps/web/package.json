{
  "name": "@yx-chat/web",
  "version": "0.0.0",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite --config ./vite.config.ts --open",
    "build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build --config ./vite.config.ts",
    "build:dev": "cross-env NODE_ENV=development vue-tsc --noEmit && vite build --mode development --config ./vite.config.ts",
    "preview:dev": "cross-env NODE_ENV=development vite preview",
    "preview:prod": "cross-env NODE_ENV=production vite preview",
    "lint": "eslint",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@hekeh/easy-audio-player-vue": "^1.0.7",
    "@yx-chat/i18n": "workspace:*",
    "@yx-chat/shared": "workspace:*",
    "artplayer": "^5.2.1",
    "axios": "^1.7.7",
    "dayjs": "^1.11.9",
    "element-plus": "^2.8.7",
    "lodash": "^4.17.21",
    "platform": "^1.3.6",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.12",
    "vue-i18n": "^9.14.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/platform": "^1.3.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/compiler-sfc": "^3.5.12",
    "sass": "^1.80.6",
    "svgo": "^3.3.2",
    "vite": "^5.4.10",
    "vue-tsc": "^2.1.10"
  },
  "browserslist": [
    "> 1%",
    "not ie 11"
  ]
}
